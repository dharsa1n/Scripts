-- Numbers only input
amountBox:GetPropertyChangedSignal("Text"):Connect(function()
    local text = amountBox.Text
    local numbersOnly = text:gsub("[^%d]", "")
    if text ~= numbersOnly then
        amountBox.Text = numbersOnly
    end
end)

